func fib(n) {
    if n < 2 {
        return n
    } else {
        return fib(n-1) + fib(n-2)
    }
}
func fact(n) {
    if n > 0 {
        return n * fact(n-1)
    }
    return 1
}
func sum(n) {
    if n > 0 {
        return n + sum(n-1)
    }
    return n
}
func loop(x) {
    for i = 0; i < x; i++ {
        print i
    }
    return sum(x)
}

print fib(11)
print fact(5)
print sum(11)
print loop(5)

